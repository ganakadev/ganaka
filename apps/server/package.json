{
  "name": "@ganaka/server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "env-cmd -f ../../.env --silent nodemon",
    "build": "env-cmd -f ../../.env --silent tsc",
    "start": "env-cmd -f ../../.env --silent node dist/index.js",
    "lint": "tsc --noEmit",
    "test": "playwright test --project=auth && playwright test --project=empty-db && playwright test --project=negative && playwright test --project=positive",
    "test:auth": "playwright test --project=auth",
    "test:empty-db": "playwright test --project=empty-db",
    "test:positive": "playwright test --project=positive",
    "test:negative": "playwright test --project=negative"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/compress": "^8.3.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/sensible": "^6.0.4",
    "@ganaka/db": "workspace:*",
    "@ganaka/schemas": "workspace:*",
    "axios": "^1.7.9",
    "cheerio": "^1.1.2",
    "dayjs": "^1.11.19",
    "fastify": "^5.2.0",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@playwright/test": "^1.48.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^22.19.3",
    "dotenv": "^16.6.1",
    "env-cmd": "^10.1.0",
    "nodemon": "^3.1.11",
    "playwright": "^1.57.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
