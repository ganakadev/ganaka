{
  "name": "@ganaka/sdk",
  "version": "1.0.1",
  "description": "Ganaka SDK - TypeScript client library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "data",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "dev": "vite build --watch",
    "lint": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "semantic-release": "semantic-release",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "ganaka",
    "sdk",
    "typescript",
    "trading",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20.10.0",
    "@vitest/ui": "^2.1.8",
    "esbuild": "^0.27.0",
    "rollup": "^4.53.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/workingsignature/ganaka.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/workingsignature/ganaka/issues"
  },
  "homepage": "https://github.com/workingsignature/ganaka#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "@ganaka/schemas": "workspace:*",
    "@ganaka/db": "workspace:*"
  },
  "peerDependencies": {
    "dotenv": "^16.4.5"
  },
  "peerDependenciesMeta": {
    "dotenv": {
      "optional": true
    }
  }
}
