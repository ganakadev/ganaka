{
  "name": "ganaka",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "pnpm -r --parallel run lint",
    "prepare:ci": "./scripts/prepare-for-ci.sh",
    "prepare:release": "./scripts/prepare-for-release.sh",
    "reset:test-db": "env-cmd -f .env --silent bash ./scripts/reset-test-db.sh",
    "test:server": "pnpm --filter @ganaka/server test",
    "clean:node_modules": "find . -name 'node_modules' -type d -prune -exec rm -rf {} +"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971",
  "dependencies": {
    "dotenv": "^17.2.3",
    "env-cmd": "^11.0.0"
  }
}
